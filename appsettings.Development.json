{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FasmgaDatabaseSettings": {
    "DatabaseName": "testing",
    "UrlsCollectionName": "urls",
    "UsersCollectionName": "users"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "DisableRateLimitHeaders": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*:/v1/create",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*:/v1/create",
        "Period": "1d",
        "Limit": 50
      },
      {
        "Endpoint": "*:/v1/urls*",
        "Period": "1d",
        "Limit": 200
      },
      {
        "Endpoint": "*:/v1/delete",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*:/v1/delete",
        "Period": "1d",
        "Limit": 50
      },
      {
        "Endpoint": "*:/v1/edit",
        "Period": "1m",
        "Limit": 5
      },
      {
        "Endpoint": "*:/v1/edit",
        "Period": "1d",
        "Limit": 25
      }
    ]
  }
}
